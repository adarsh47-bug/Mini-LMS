{
  "name": "mlms",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "doctor": "npx expo-doctor --verbose",
    "eas:build": "npm run doctor && eas build --profile production --platform all",
    "eas:build:android": "npm run doctor && eas build --profile production --platform android",
    "eas:build:ios": "npm run doctor && eas build --profile production --platform ios",
    "eas:dev": "npm run doctor && eas build --profile development --platform all",
    "eas:dev:update": "npm run doctor && eas update --channel development --platform all",
    "eas:dev:android": "npm run doctor && eas build --profile development --platform android",
    "eas:dev:update:android": "npm run doctor && eas update --channel development --platform android",
    "eas:dev:ios": "npm run doctor && eas build --profile development --platform ios",
    "eas:dev:update:ios": "npm run doctor && eas update --channel development --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@legendapp/list": "^2.0.19",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-application": "^7.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.2",
    "expo-dev-client": "^6.0.20",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-navigation-bar": "^5.0.10",
    "expo-network": "^8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "^29.0.16",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-test-renderer": "19.1.0",
